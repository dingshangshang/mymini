.navbar {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
}

.return,.main{
  padding: 10rpx;
  margin-top: -5rpx;
}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 150rpx;
  padding-right: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  font-size: 16px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navbar-action-wrap {
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: absolute;
  left: 10px;
  z-index: 11;
  line-height: 1;
  padding-top: 4px;
  padding-bottom: 4px;
}
.iconfont{
  font-size: 38rpx;
}